{
  "name": "mixdown",
  "version": "1.0.0",
  "description": "a app for building music sites ",
  "main": "index.js",
  "scripts": {
    "start:server": "nodemon -e js,jsx webroot/server/index.js",
    "test": "jest --watch",
    "build": "webpack --mode=production",
    "start:dev": "webpack-dev-server --mode=development --open",
    "start:cdn": "nodemon webroot/cdn/index.js",
    "start:db": "./start-db.sh",
    "stop:db": " mongod --quiet",
    "start": "concurrently \"yarn start:cdn \" \"yarn start:dev \" \"yarn start:server \" \"yarn start:db \""
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/fizzyliquid/mixdown.git"
  },
  "keywords": [
    "music"
  ],
  "author": "Trevor Joseph",
  "license": "ISC",
  "homepage": "https://bitbucket.org/fizzyliquid/mixdown#readme",
  "devDependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "copy-webpack-plugin": "^4.6.0",
    "file-loader": "^2.0.0",
    "graphql-tag": "^2.10.0",
    "jest-localstorage-mock": "^2.4.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "apollo-boost": "^0.1.20",
    "apollo-cache-inmemory": "^1.3.9",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "apollo-server": "^2.25.4",
    "apollo-server-express": "^2.2.5",
    "apollo-upload-client": "^10.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "body-parser": "^1.18.3",
    "classname": "^0.0.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "css-loader": "^1.0.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "es6-promise": "^4.2.5",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "fetch-mock": "^7.2.5",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.16.9",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "jest-express": "^1.10.1",
    "jest-fetch-mock": "^1.6.6",
    "jest-styled-components": "^6.3.1",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "localstorage-polyfill": "^1.0.1",
    "lodash.camelcase": "^4.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mock-fs": "^4.10.1",
    "mockingoose": "^2.11.0",
    "moment": "^2.24.0",
    "mongoose": "^5.3.10",
    "moxios": "^0.4.0",
    "multer": "^1.4.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.18.6",
    "peaks": "^1.0.1",
    "png-to-jpeg": "^1.0.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.8.0",
    "react-apollo": "^2.2.4",
    "react-cropper": "^1.2.0",
    "react-dom": "^16.8.0",
    "react-dropzone": "^8.0.3",
    "react-mock-router": "^1.0.15",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-soundplayer": "^1.0.4",
    "regenerator-runtime": "^0.12.1",
    "styled-components": "^4.0.3",
    "supertest": "^4.0.2",
    "unfetch": "^4.0.1",
    "wavesurfer": "^1.3.4",
    "webpack-dev-server": "^3.1.9"
  },
  "jest": {
    "automock": false,
    "collectCoverageFrom": [
      "webroot/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTest.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "<rootDir>/webroot/**/__tests__/**/*.js?(x)",
      "<rootDir>/webroot/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  }
}
