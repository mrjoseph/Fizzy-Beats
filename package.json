{
  "name": "mixdown",
  "version": "1.0.0",
  "description": "a app for building music sites ",
  "main": "index.js",
  "scripts": {
    "server": "nodemon -e js,jsx webroot/server/index.js",
    "test": "jest",
    "build": "webpack --mode=production",
    "start": "webpack-dev-server --mode=development --open"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/fizzyliquid/mixdown.git"
  },
  "keywords": [
    "music"
  ],
  "author": "Trevor Joseph",
  "license": "ISC",
  "homepage": "https://bitbucket.org/fizzyliquid/mixdown#readme",
  "devDependencies": {
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "css-loader": "^1.0.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "mini-css-extract-plugin": "^0.4.4",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "webpack-dev-server": "^3.1.9"
  },
  "jest": {
    "collectCoverageFrom": [
      "webroot/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTest.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/webroot/**/__tests__/**/*.js?(x)",
      "<rootDir>/webroot/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  }
}
